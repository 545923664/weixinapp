<view wx:for="{{list}}">
<view style="width:98%;margin-left:10rpx;height:200rpx;margin-top:10px;">
    <view style="float:left;width:10%;">
       <image  style="width:130rpx;height:160rpx;margin-left:15rpx;"
                 src="http://p1.pstatp.com/list/190x124/11980006f42f57a8d8f0"/>
    </view>
    <view style="margin-left:145rpx;width:88%;height:200rpx;">
        <view style="width:100%;height:60rpx;margin-right:10rpx;">
            <view style="width:75%;height:60rpx;border:1rpx solid;solid:black;display:inline;text-align:center;">
                {{item.releasetime}}
            </view>
            <view style="width:25%;height:60rpx;border:1rpx solid;solid:black;display:inline;text-align:center;">
                系统消息
            </view>
        </view>
        <view style="width:87.6%;height:140rpx;border:1rpx solid;solid:black;display:block;text-align:center;margin-top:-15rpx;">
            {{item.announcetitle}}
        </view>
    </view>
</view>




