<view wx:for="{{list}}">
  <view class="section10">
    <view class="flex-wrp1s" style="flex-direction:row;">
      <view class="flex-item6">
        <image style="height:120rpx;" src="http://p1.pstatp.com/list/190x124/11980006f42f57a8d8f0" />
      </view>
      <view class="section11">
        <view class="flex-wrp1ps" style="flex-direction:column;">
          <view class="flex-wrp1p" style="flex-direction:row;">
            <view class="flex-item61">
              {{item.roomname}}
            </view>
            <view class="flex-item62">
              {{item.roompeoplenum}}
            </view>
          </view>
          <view class="flex-item63">
            {{item.roomtitle}}
          </view>
        </view>
      </view>
      <view class="buttona" bindtap="clickMe" data-tId="{{item.id}}">
        <!-- <button>加入游戏</button>-->
        <navigator url="../gameOne/gameOne">加入 游戏</navigator>
      </view>
    </view>
  </view>
